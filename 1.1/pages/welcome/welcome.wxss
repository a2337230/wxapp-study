.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 200rpx;
}
.avatar {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  display: block;
  margin-bottom: 100rpx;
}
.title {
  font-size: 40rpx;
}
.btn {
  margin-top: 200rpx;
  border: 1px solid blue;
  border-radius: 10rpx;
  padding: 30rpx;
}